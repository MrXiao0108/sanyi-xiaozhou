<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.dzics.data.appoint.changsha.mom.db.dao.WmsOrderConfigDao">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.dzics.data.appoint.changsha.mom.model.entity.WmsOrderConfig">
        <id column="config_order_id" property="configOrderId"/>
        <result column="rfid" property="rfid"/>
        <result column="order_num" property="orderNum"/>
        <result column="materialCode" property="materialCode"/>
        <result column="org_code" property="orgCode"/>
        <result column="del_flag" property="delFlag"/>
        <result column="create_by" property="createBy"/>
        <result column="create_time" property="createTime"/>
        <result column="update_by" property="updateBy"/>
        <result column="update_time" property="updateTime"/>
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        config_order_id, rfid, order_num, materialCode, org_code, del_flag, create_by, create_time, update_by, update_time
    </sql>
    <select id="getCfg" resultType="com.dzics.data.appoint.changsha.mom.model.dto.wms.GetOrderCfig">
        SELECT
        config_order_id AS configOrderId,
        rfid,
        order_status as orderStatus,
        order_num as orderNum,
        material_code as materialCode,
        create_time as createTime
        FROM
        wms_order_config
        ORDER BY
        create_time DESC
    </select>

</mapper>
